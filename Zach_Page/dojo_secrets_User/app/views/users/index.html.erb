<table >
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Actions</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>

      <td>
        <a href="users/<%= user.id %>">Show</a>
        <a href="/users/<%= user.id %>/edit">Edit</a>
        <a href="/users/<%= user.id %>" data-method="delete">Destroy</a>

      </td>
    </tr>
  <%end%>
</table>


<a href="/users/new">New User</a>
